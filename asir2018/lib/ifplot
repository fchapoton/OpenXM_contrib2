def plot_reverse(F,X,A,Y,B)
{
	T = uc();
	G = nm(subst(F,X,X/T+A,Y,-Y/T+B));
	while ( 1 ) {
		if ( !(Q = tdiv(G,T)) )
			break;
		else
			G = Q;
	}
	return subst(G,T,X^2+y^2);
}
end;
